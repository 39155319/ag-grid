{
  "name": "@ag-grid-community/vue",
  "description": "ag-Grid Vue Component",
  "version": "24.0.0",
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "MIT",
  "main": "main.js",
  "typings": "main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "homepage": "http://www.ag-grid.com/",
  "keywords": [
    "grid",
    "data",
    "table",
    "vue",
    "vuejs"
  ],
  "scripts": {
    "bundle": "vue-cli-service build --target lib src/AgGridVue.ts",
    "lib": "tsc  -p tsconfig-lib.json",
    "copy-legacy": "npx gulp copy-build-artefacts-from-legacy",
    "build": "npm run lib && npm run bundle && rm ./dist/demo.html && npm run copy-legacy && npm run hash",
    "lint": "vue-cli-service lint",
    "hash": "sh ../../scripts/hashDirectory.sh > .hash"
  },
  "devDependencies": {
    "@ag-grid-community/core": "~24.0.0",
    "@types/node": "12.12.22",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.0",
    "typescript": "^3.6.0",
    "vue": "3.0.0",
    "vue-property-decorator": "9.0.0",
    "core-js": "^3.6.5",
    "vue-class-component": "^8.0.0-beta.3",
    "@ag-grid-community/vue-legacy": "~24.0.0",
    "@vue/composition-api": "^1.0.0-beta.14",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "@ag-grid-community/core": "~24.0.0",
    "vue": "^2.6.12 || ^3.0.0",
    "vue-property-decorator": "^9.0.0",
    "vue-class-component": "~7.2.6 || >= ^8.0.0-beta.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
